#!/bin/sh
cp /usr/share/Dagcoin-wallet@type/dagcoin-wallet@type.desktop /usr/share/applications/dagcoin-wallet@type.desktop
for user in /home/*
do
    cp /usr/share/applications/dagcoin-wallet@type.desktop ${user}/Desktop/dagcoin-wallet@type.desktop
    chmod +x ${user}/Desktop/dagcoin-wallet@type.desktop
done
